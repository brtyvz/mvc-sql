
@{
    ViewBag.Title = " Urun Sil";
}

@using Ornek.Models
@model Urunler 

<div class="row">
    <div class="col-md-12">

<div class="block">

    <div class="block heading">
        <div class="main-text h2">
            Urun Silme Sayfasi

        </div>
        <div class="block-controls">
            <a href="/Urun/Index"><span aria-hidden="true" class="icon icon-cross icon-size-medium block-control-remove"></a>
            <span aria-hidden="true" class="icon icon-arrow-down icon-size-medium block-control-collapse"></span>
        </div>
    </div>
    <div class="block-content-outer">
        <div class="block-content-inner">
            <form action="~/Urun/Sil" method="post" >
                <input type="hidden" name="UrunID"value="@Model.UrunID" />
                <label >@Model.UrunAdi urunu silinsin mi? </label>
                <input class="btn btn-danger" type="submit"name="name"value="evet" />
            </form>
        </div>
    </div>

</div>

    </div>

</div>

